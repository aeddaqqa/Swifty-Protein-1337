{
  "expo": {
    "name": "Swifty-Protein",
    "slug": "Swifty-Protein",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Psplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kzouggar.Swifty-Protein",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Swifty-proteins needs to use Face ID / Touch ID to authenticate you"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
